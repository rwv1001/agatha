<h1>Editing attendee</h1>

<% form_for(@attendee) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :lecture_id %><br />
    <%= f.text_field :lecture_id %>
  </p>
  <p>
    <%= f.label :person_id %><br />
    <%= f.text_field :person_id %>
  </p>
  <p>
    <%= f.label :examined %><br />
    <%= f.check_box :examined %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @attendee %> |
<%= link_to 'Back', attendees_path %>
